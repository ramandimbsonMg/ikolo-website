generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// Mod√®le Student
model Student {
  id        Int      @id @default(autoincrement())
  nom       String
  prenom    String
  naissance DateTime
  sexe      String
  email     String   @unique
  telephone String?
  adresse   String?
  programme String?
  matieres  String?
  createdAt DateTime @default(now())
}
